{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "healthCheckConfiguration": [
    {
      "Id": "cf-backend",
      "Name": "CF Backend",
      "Type": "Http",
      "Target": "https://google.com",
      "ExpectedResponseCode": 200,
      "Method": "GET",
      "TimeoutSeconds": 10,
      "IntervalMinutes": 1
    },
    {
      "Id": "audit-logger",
      "Name": "Audit Logger",
      "Type": "Http",
      "Target": "https://ziglang.org",
      "ExpectedResponseCode": 200,
      "Method": "GET",
      "TimeoutSeconds": 10,
      "IntervalMinutes": 1
    },
    {
      "Id": "maf-notifier",
      "Name": "Maf Notifier",
      "Type": "Http",
      "Target": "https://www.rust-lang.org",
      "ExpectedResponseCode": 200,
      "Method": "GET",
      "TimeoutSeconds": 10,
      "IntervalMinutes": 1
    }
  ]
}
